{
  "name": "rocketagent",
  "version": "1.0.0",
  "description": "Rocket Science AI receptionist test API",
  "main": "apps/api/dist/index.js",
  "scripts": {
    "build": "npm run build:api",
    "start": "npm run start:api",
    "seed": "npm run prisma:seed",
    "build:api": "tsc -p apps/api/tsconfig.json",
    "dev:api": "tsc -p apps/api/tsconfig.json --watch",
    "start:api": "node apps/api/dist/index.js",
    "prisma:generate": "prisma generate --schema apps/api/prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema apps/api/prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema apps/api/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema apps/api/prisma/schema.prisma",
    "prisma:db:push": "prisma db push --schema apps/api/prisma/schema.prisma",
    "prisma:seed": "prisma db seed --schema apps/api/prisma/schema.prisma",
    "build:widget": "tsc -p apps/widget/tsconfig.json",
    "dev:widget": "tsc -p apps/widget/tsconfig.json --watch"
  },
  "prisma": {
    "schema": "apps/api/prisma/schema.prisma",
    "seed": "node apps/api/prisma/seed.cjs"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "axios": "^1.7.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.16.0",
    "prisma": "^7.1.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "24.x"
  }
}
